# Příklad dobře specifikovaného konečného automatu ve vstupním formátu úlohy MKA (IFJ04, snímek 40/44) obohacený o past trap
({s, f, q1, q2, q3, q4, trap}, 
 {'a', 'b'}, 
 {s 'a' -> trap, f 'a' -> s , s 'b' -> q3, f 'b' -> q4,
  q1 'a' -> q1, q1 'b' -> s, q3 'b' -> q1, q3 'a' -> q4, 
  q4 'a' -> q3, q4 'b' -> q2, q2 'a' -> q2, q2 'b' -> f,
  trap 'a' -> trap, trap 'b' -> trap}, 
  s, 
 {f, s}
)
